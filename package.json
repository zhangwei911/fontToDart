{
	"name": "fonttodartts",
	"displayName": "fontToDartTS",
	"description": "将阿里云iconfont.json转换成Dart",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "wellchang",
	"activationEvents": [
		"onCommand:fonttodartts.fontToDart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fonttodartts.fontToDart",
				"title": "fontToDart"
			}
		],
		"configuration": [
			{
				"title": "Font To Dart",
				"properties": {
					"vscodePluginDemo.yourName": {
						"type": "string",
						"default": "guest",
						"description": "你的名字"
					},
					"vscodePluginDemo.showTip": {
						"type": "boolean",
						"default": true,
						"description": "是否在每次启动时显示欢迎提示！"
					}
				}
			}
		]
	},
	"keywords": [
		"flutter",
		"icon",
		"font"
	],
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.6.4",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0",
		"vscode": "^1.1.37"
	},
	"dependencies": {
		"urijs": "^1.19.2",
		"vsce": "^1.79.5"
	}
}
